/* pages/key/key.wxss */
.page {
    padding-bottom: 200rpx;
}

.btn {
    position: fixed;
    bottom: 0;
    width: calc(100% - 60rpx);
    padding: 20rpx 30rpx;
    padding-bottom: env(safe-area-inset-bottom);
    background-color: #ffffff;
}

.btn>view {
    width: 100%;
    height: 80rpx;
    font-size: 28rpx;
    color: #ffffff;
    line-height: 80rpx;
    text-align: center;
    background-color: #4e6ef2;
    border-radius: 10rpx;
}

.content {
    margin-bottom: 30rpx;
}

.title {
    font-size: 36rpx;
    font-weight: 600;
}

.num {
    padding-right: 10rpx;
    color: #4e6ef2;
}

image {
    display: block;
    margin: 0 auto;
}

.qc1 {
    height: 240rpx;
}

.qc2 {
    height: 300rpx;
}

.qc3 {
    height: 700rpx;
}